{
  "name": "_OLLIE_NAME_",
  "description": "_OLLIE_DESCRIPTION_",
  "version": "1.0.0",
  "main": "index.js",
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "build": "yarn cleanup && tsc",
    "cleanup": "rm -rf ./build",
    "serve": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest __tests__/* -- --coverage",
    "test:watch": "jest --watchAll",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "yarn db:migrate:undo && yarn db:migrate && yarn db:migrate && yarn db:seed"
  },
  "repository": {
    "url": "git@github.com:icapps/nodejs-express-template.git",
    "type": "git"
  },
  "contributors": [
    {
      "name": "Brent van Geertruy",
      "email": "brent.vangeertruy@icapps.com"
    },
    {
      "name": "Olivier Van den Mooter",
      "email": "olivier.vandenmooter@icapps.com"
    },
    {
      "name": "Samuel Overloop",
      "email": "samuel.overloop@icapps.com"
    },
    {
      "name": "Robin Houdmeyers",
      "email": "robin.houdmeyers@icapps.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "dotenv-safe": "^4.0.4",
    "jest": "^20.0.4",
    "moment": "^2.18.1",
    "pg": "^6.4.0",
    "raven": "^2.1.0",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^4.2.0",
    "sequelize-cli": "^2.7.0",
    "sequelize-typescript": "^0.3.5",
    "tree-house": "0.1.5",
    "ts-jest": "^20.0.6",
    "ts-node": "^3.0.6",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.0",
    "@types/jest": "^20.0.2",
    "@types/node": "^8.0.1",
    "@types/sequelize": "^3.4.52",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "tslint-config-airbnb": "^5.2.0",
    "typescript": "^2.3.4"
  }
}
